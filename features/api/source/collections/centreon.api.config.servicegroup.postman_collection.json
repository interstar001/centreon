    "name": "List service group",
    "event": [{
      "listen": "test",
      "script": {
        "type": "text\/javascript",
        "exec": ["var jsonData = JSON.parse(responseBody);",
          "var sgData = jsonData.result;",
          "",
          "try {",
          "    tests[\"Body contains list of service groups\"] = sgData;",
          "    var i = 0;",
          "    while (i < sgData.length) {",
          "        if (postman.getEnvironmentVariable(\"sg_name\") == sgData[i].name)",
          "        {",
          "            tests[\"Body contains added service group\"] = postman.getEnvironmentVariable(\"sg_name\") == sgData[i].name;",
          "            tests[\"Body contains added service alias\"] = postman.getEnvironmentVariable(\"sg_alias\") == sgData[i].alias;",
          "            break;",
          "        }",
          "        i++;",
          "    }",
          "    if (i == sgData.length)",
          "        tests[\"sg_name was found\"] = false;",
          "}",
          "catch (e) {}",
          "",
          "tests[\"Status code is 200\"] = responseCode.code === 200;"]
      }
    }],
