    "name": "List service category",
    "event": [{
      "listen": "test",
      "script": {
        "type": "text\/javascript",
        "exec": ["var jsonData = JSON.parse(responseBody);",
          "var scData = jsonData.result;",
          "",
          "try {",
          "    tests[\"Body contains list of service categories\"] = scData;",
          "    var i = 0;",
          "    while (i < scData.length) {",
          "        if (postman.getEnvironmentVariable(\"sc_name\") == scData[i].name)",
          "        {",
          "            tests[\"Body contains added service category\"] = postman.getEnvironmentVariable(\"sc_name\") == scData[i].name;",
          "            tests[\"Body contains added description\"] = postman.getEnvironmentVariable(\"sc_description\") == scData[i].description;",
          "            break;",
          "        }",
          "        i++;",
          "    }",
          "    if (i == scData.length)",
          "        tests[\"sc_name was found\"] = false;",
          "}",
          "catch (e) {}",
          "",
          "tests[\"Status code is 200\"] = responseCode.code === 200;"]
      }
    }],
