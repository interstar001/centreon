    "name": "List traps",
    "event": [{
      "listen": "test",
      "script": {
        "type": "text\/javascript",
        "exec": ["var jsonData = JSON.parse(responseBody);",
          "var trapData = jsonData.result;",
          "",
          "try {",
          "    tests[\"Body contains list of matchings\"] = trapData;",
          "    var i = 0;",
          "    while (i < trapData.length) {",
          "        if (postman.getEnvironmentVariable(\"trap_name\") == trapData[i].name) {",
          "            tests[\"Body contains added trap\"] = postman.getEnvironmentVariable(\"trap_name\") == trapData[i].name;",
          "            tests[\"Body contains added trap oid\"] = postman.getEnvironmentVariable(\"trap_oid\") == trapData[i].oid;",
          "            break;",
          "        }",
          "        i++;",
          "    }",
          "    if (i == trapData.length)",
          "        tests[\"trap_name was found\"] = false;",
          "} catch (e) {}",
          "",
          "tests[\"Status code is 200\"] = responseCode.code === 200;"]
      }
    }],
