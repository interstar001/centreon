    "name": "List dependencies",
    "event": [{
      "listen": "test",
      "script": {
        "type": "text\/javascript",
        "exec": ["var jsonData = JSON.parse(responseBody);",
          "var depData = jsonData.result;",
          "",
          "try {",
          "    tests[\"Body contains list of dependencies\"] = depData;",
          "    var i = 0;",
          "    while (i < depData.length) {",
          "        if (postman.getEnvironmentVariable(\"dep_name\") == depData[i].name)",
          "        {",
          "            tests[\"Body contains added dep_name\"] = postman.getEnvironmentVariable(\"dep_name\") == depData[i].name;",
          "            tests[\"Body contains added dep_description\"] = postman.getEnvironmentVariable(\"dep_description\") == depData[i].description;",
          "            tests[\"Body contains added dep_inherits_parents\"] = postman.getEnvironmentVariable(\"dep_inherits_parent\") == depData[i].inherits_parent;",
          "            tests[\"Body contains added dep_execution_failure_criteria\"] = postman.getEnvironmentVariable(\"dep_exec_fail_crit\") == depData[i].execution_failure_criteria;",
          "            tests[\"Body contains added dep_notification_failure_criteria\"] = postman.getEnvironmentVariable(\"dep_notif_fail_crit\") == depData[i].notification_failure_criteria;",
          "            break;",
          "        }",
          "        i++;",
          "    }",
          "    if (i == depData.length)",
          "        tests[\"dep_name was found\"] = false;",
          "}",
          "catch (e) {}",
          "",
          "tests[\"Status code is 200\"] = responseCode.code === 200;"]
      }
    }],
