          "        {",
          "            tests[\"Body contains added cg_name\"] = postman.getEnvironmentVariable(\"cg_name\") == cgData[i].name;",
          "            tests[\"Body contains added cg_alias\"] = postman.getEnvironmentVariable(\"cg_alias\") == cgData[i].alias;",
          "            break;",
          "        }",
          "        i++;",
          "    }",
          "    if (i == cgData.length)",
          "        tests[\"cg_name was found\"] = false;",
          "}",
          "catch (e) {}",
          "",
          "tests[\"Status code is 200\"] = responseCode.code === 200;"]
