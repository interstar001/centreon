    "name": "List host groups",
    "event": [{
      "listen": "test",
      "script": {
        "type": "text\/javascript",
        "exec": ["var jsonData = JSON.parse(responseBody);",
          "var hgData = jsonData.result;",
          "",
          "try {",
          "    tests[\"Body contains list of host categories\"] = hgData;",
          "    var i = 0;",
          "        while (i < hgData.length) {",
          "        if (postman.getEnvironmentVariable(\"hg_name\") == hgData[i].name)",
          "        {",
          "            tests[\"Body contains added hg_name\"] = postman.getEnvironmentVariable(\"hg_name\") == hgData[i].name;",
          "            tests[\"Body contains added hg_alias\"] = postman.getEnvironmentVariable(\"hg_alias\") == hgData[i].alias;",
          "            break;",
          "        }",
          "        i++;",
          "    }",
          "    if (i == hgData.length)",
          "        tests[\"hg_name was found\"] = false;",
          "}",
          "catch (e) {}",
          "",
          "tests[\"Status code is 200\"] = responseCode.code === 200;"]
      }
    }],
